	HasInfluence Shaper
	Rarity Normal Magic Rare
	Class "Maps"

Show # $type->maps->influenced $tier->infelder
	HasInfluence Elder
	Rarity Normal Magic Rare
	Class "Maps"

Show # $type->maps->influenced $tier->infconquerors
	HasInfluence Crusader Hunter Redeemer Warlord
	Rarity Normal Magic Rare
	Class "Maps"

