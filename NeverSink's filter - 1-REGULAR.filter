	BaseType "Sorcerer Boots"
	Rarity Normal
	SetFontSize 38
	SetTextColor 255 255 255 255         # TEXTCOLOR:	 Normal Items: Strong Highlight
	SetBorderColor 0 210 0 210           # BORDERCOLOR:	 T1 Chancing
