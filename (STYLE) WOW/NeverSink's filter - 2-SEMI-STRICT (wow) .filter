# !! Chapter "High Priority Rules"
# !! Override 010 : "ALL Rules"

Show # $type->6l $tier->arm
	Corrupted False
	LinkedSockets 6
	Rarity <= Rare
	Class "Body Armours"
	SetFontSize 45
	SetTextColor 255 128 0 255
	SetBorderColor 255 128 0 255
	SetBackgroundColor 255 255 255 255
	PlayAlertSound 6 300
	PlayEffect Red
	MinimapIcon 0 Red Star

Show # $type->6l $tier->wep
	LinkedSockets 6
	Rarity <= Rare
	SetFontSize 45
	SetTextColor 255 128 0 255
	SetBorderColor 255 128 0 255
	SetBackgroundColor 0 0 0 255
	PlayAlertSound 1 300
	PlayEffect Orange
	MinimapIcon 0 Orange Star

